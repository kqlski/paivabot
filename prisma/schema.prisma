datasource db {
  // could be postgresql or mysql
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator db {
  provider  = "prisma-client-py"
  interface = "asyncio"
}

model WeatherIcon {
  code      Int
  temperature Int
  votes_yes    Int @default(1)
  votes_no      Int  @default(1)
  @@id([code,temperature])
}